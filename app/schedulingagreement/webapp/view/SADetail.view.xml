<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="sp.fiori.schedulingagreement.controller.SADetail">
	<Page id="_IDGenPage1" title="Schedule Agreement : {detailModel>/Schedule_No}" class="sapUiSizeCompact" titleAlignment="Center">
		<content>
			<!-- <ObjectHeader id="ObjectId"
				title="Upcoming Delivery : {path:'detailModel>/Upcoming_Del_Date', formatter:'sp.fiori.schedulingagreement.controller.formatter.formatDate'}"
				number="{detailModel>/Amount}" numberUnit="{detailModel>/Currency}" numberState="Success" backgroundDesign="Translucent"> -->
				<ObjectHeader id="ObjectId">
				<attributes>
					<ObjectAttribute id="_IDGenObjectAttribute2" text="Vendor: {detailModel>/Vendor_Name}"/>
					<ObjectAttribute id="_IDGenObjectAttribute1" text="Plant: {detailModel>/Plant_Desc} ({detailModel>/Plant})"/>
				</attributes>
				<statuses>
					<ObjectStatus id="_IDGenObjectStatus1" text="{path:'detailModel>/Schedule_Date', formatter:'sp.fiori.schedulingagreement.controller.formatter.formatDate'}"/>
				</statuses>
			</ObjectHeader>
			<IconTabBar id="_IDGenIconTabBar1">
				<items>
					<IconTabFilter id="_IDGenIconTabFilter1" icon="sap-icon://approvals" text="Items">
						<Table id="DeliveryTableId" items="{detailModel>/Sitemnav}" itemPress="onItempress">
							<headerToolbar>
								<Toolbar id="_IDGenToolbar1">
									<Title id="_IDGenTitle1" text="Items"/>
								</Toolbar>
							</headerToolbar>
							<columns>
								<Column id="_IDGenColumn1" minScreenWidth="Tablet" demandPopin="true" hAlign="Begin">
									<Text id="_IDGenText1" text="Item No."/>
								</Column>
								<!--<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Begin">-->
								<!--	<Text text="Schedule Line"/>-->
								<!--</Column>-->
								<Column id="_IDGenColumn2" minScreenWidth="Tablet" demandPopin="true" width="200px">
									<Text id="_IDGenText2" text="Material"/>
								</Column>
								<!--<Column minScreenWidth="Tablet" demandPopin="true" width="200px">-->
								<!--	<Text text="Material Group"/>-->
								<!--</Column>-->
								<Column id="_IDGenColumn3" minScreenWidth="Tablet" demandPopin="true" width="200px">
									<Text id="_IDGenText3" text="HSN Code"/>
								</Column>
								<Column id="_IDGenColumn4" minScreenWidth="Tablet" demandPopin="true">
									<Text id="_IDGenText4" text="Delivery Schedule"/>
								</Column>
								<Column id="_IDGenColumn5" minScreenWidth="Tablet" demandPopin="true">
									<Text id="_IDGenText5" text="Delivered Quantity"/>
								</Column>
								<Column id="_IDGenColumn6" minScreenWidth="Tablet" demandPopin="true">
									<Text id="_IDGenText6" text="Unit Price"/>
								</Column>
								<Column id="_IDGenColumn7" minScreenWidth="Tablet" demandPopin="true">
									<Text id="_IDGenText7" text="Status"/>
								</Column>
								<!-- <Column id="_IDGenColumn8" hAlign="End" width="20px">
									<Text id="_IDGenText8"/>
								</Column> -->
							</columns>
							<items>
								<ColumnListItem id="_IDGenColumnListItem1" type="Navigation">
									<cells>
										<!--<CheckBox selected="{detailModel>Item_indicator}" select="onChkBoxSelect"-->
										<!--	enabled="{path:'detailModel>Confirm_Status',formatter:'sp.fiori.schedulingagreement.controller.formatter.selectEnabled'}"></CheckBox>-->
										<Text id="_IDGenText9" text="{detailModel>Item_No}"/>
										<!--<Text text="{detailModel>Etenr}"/>-->
										<FlexBox id="_IDGenFlexBox1" direction="Column" class="sapUiNoMarginTop">
											<items>
												<Text id="_IDGenText10" text="{detailModel>Material_Desc}"/>
												<!--<Link text="{detailModel>Material_No}" press="onMaterialPress" emphasized="true" enabled="{path:'detailModel>Matkl',formatter:'sp.fiori.schedulingagreement.controller.formatter.LinkEnabled'}"/>-->
												<Link id="_IDGenLink1" text="{detailModel>Material_No}" press="onMaterialPress" emphasized="true" enabled="true"/>
												<!--<ObjectIdentifier title="{detailModel>Material_No}"/>-->
											</items>
										</FlexBox>
										<!--<FlexBox direction="Column" class="sapUiNoMarginTop">-->
										<!--	<items>-->
										<!--		<Text text="{detailModel>Matkl}"/>-->
										<!--		<ObjectIdentifier title="{detailModel>Wgbez}"/>-->
										<!--	</items>-->
										<!--</FlexBox>-->
										<Text id="_IDGenText11" text="{detailModel>Hsn_Code}"></Text>
										<ObjectNumber id="_IDGenObjectNumber1" number="{detailModel>Quantity}" numberUnit="{detailModel>Uom}"/>
										<ObjectNumber id="_IDGenObjectNumber2" number="{detailModel>Delivered_Qty}" numberUnit="{detailModel>Uom}"/>
										<ObjectNumber id="_IDGenObjectNumber3" number="{detailModel>Netpr}"/>
										<ObjectStatus id="_IDGenObjectStatus2" text="{detailModel>Confirm_Status}"
											state="{path:'detailModel>Confirm_Status',formatter:'sp.fiori.schedulingagreement.controller.formatter.statusCheck'}"/>
										<!-- <core:Icon id="_IDGenIcon1" src="sap-icon://feeder-arrow" color="Blue"/> -->
									</cells>
								</ColumnListItem>
							</items>
						</Table>
					</IconTabFilter>
				</items>
			</IconTabBar>
		</content>
		<footer>
			<Toolbar id="_IDGenToolbar2">
				<!--<Button text="Create ASN" press="onCreateAsn" class="sapUiTinyMarginEnd" icon="sap-icon://add" type="Emphasized"-->
				<!--	visible="{= ${detailModel>Status} === 'New'? false : true}"/>-->
				<Button id="_IDGenButton1" text="Create ASN" press="onCreateAsn" class="sapUiTinyMarginEnd" icon="sap-icon://add" type="Emphasized"
					visible="{parts:[{path:'detailModel>/Status'},{path:'detailModel>/Validity'}],formatter:'sp.fiori.schedulingagreement.controller.formatter.asnCreateVisible'}"
					enabled="{path:'detailModel>/Asn_Flag',formatter:'sp.fiori.schedulingagreement.controller.formatter.createASNBtn'}"/>
				<ToolbarSpacer id="_IDGenToolbarSpacer1"/>
				<Button id="_IDGenButton2" text="Confirm" press="onCofirmAsn" class="sapUiTinyMarginEnd" icon="sap-icon://complete" type="Emphasized"
					visible="{path:'detailModel>/Validity',formatter:'sp.fiori.schedulingagreement.controller.formatter.confirmBtnVisible'}"
					enabled="{path:'detailModel>/Sa_Flag',formatter:'sp.fiori.schedulingagreement.controller.formatter.createASNBtn'}"/>
			</Toolbar>
		</footer>
	</Page>
</mvc:View>